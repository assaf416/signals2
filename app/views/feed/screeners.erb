<h1> Screeners </h1><hr/>
<div class="card">
  <div class="card-header">
    Header
  </div>
  <div class="card-body">
<table class="table table-hover">
    <thead>
      <tr>
        <th>Screener </th>
        <th>Symbols</th>
      </tr>
    </thead>
    <tbody>
        <% for s in @screeners do %>
      <tr>
        <td><%= s[:name]%></td>
        <td><%= s[:results]%></td>
      </tr>
      <% end %>
      
    </tbody>
  </table>
   </div>
  <div class="card-footer">
    Footer
  </div>
</div>
